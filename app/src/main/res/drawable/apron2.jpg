  function addFreeGiftCard(total_price, ispaidgiftfound, isfreegiftfound, ischecked, giftwrapmessage, giftwrapfreevariantid, giftwrappaidvariantid, ispaidgiftid, isfreegiftid, f) {
    if (ispaidgiftfound) {
      if (ischecked) {
        if (total_price >= 50000) {
          // remove paid gift. add free gift, update message
         // console.log('remove paid gift. add free gift, update message');
          Shopify.removeItem(ispaidgiftid, function(){
            Shopify.addItem(giftwrapfreevariantid,1, function(){
              updateGiftWrapMessage(giftwrapmessage, f);
            });
          });
        } else {
          // remove free gift. update message
        //  console.log('remove free gift. update message');
          Shopify.removeItem(isfreegiftid, function(){
            updateGiftWrapMessage(giftwrapmessage, f);
          });
        }
      } else {
        // remove paid gift and message
       // console.log('remove paid gift and message');
        Shopify.removeItem(ispaidgiftid, function(){
          updateGiftWrapMessage('', f);
        });
      }
    }

    if (isfreegiftfound) {
      if (ischecked) {
        if (total_price >= 50000) {
          // remove paid gift. update message
         // console.log('remove paid gift. update message');
          Shopify.removeItem(ispaidgiftid, function(){
            updateGiftWrapMessage(giftwrapmessage, f);
          });
        } else {
          // remove free gift. add paid gift, update message
         // console.log('remove free gift. add paid gift, update message');
          Shopify.removeItem(isfreegiftid, function(){
            Shopify.addItem(giftwrappaidvariantid,1, function(){
              updateGiftWrapMessage(giftwrapmessage, f);
            });
          });
        }
      } else {
        // remove free gift and message
       // console.log('remove free gift and message');
        Shopify.removeItem(isfreegiftid, function(){
          updateGiftWrapMessage('', f);
        });
      }
    }

    if (!isfreegiftfound && !ispaidgiftfound) {
      if (ischecked) {
        if (total_price >= 50000) {
          // add free gift. update message
         // console.log('add free gift. update message');
          Shopify.addItem(giftwrapfreevariantid,1, function(){
            updateGiftWrapMessage(giftwrapmessage, f);
          });
        } else {
          // add paid gift. update message
         // console.log('add paid gift. update message');
          Shopify.addItem(giftwrappaidvariantid,1, function(){
            updateGiftWrapMessage(giftwrapmessage, f);
          });
        }
      } else {
        // remove message
        updateGiftWrapMessage('', f);
      }
    }
  }
  

