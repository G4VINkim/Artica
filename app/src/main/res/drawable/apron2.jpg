<script type="lazyload2">
	var supported_countries = ["CA,US"];
	var current_site = "US"
window.deferredFunctions.push(function() {
	var redirectCookie = Cookies.get('country_redirect');
	var redirect = redirectCookie !== 'false' ? true : false;
	var cookiecountry_code;
  if (redirect) {
    geoip2.country(function(response) {
			var country = response.country;
			Cookies.set('country_code', country.iso_code);
			cookiecountry_code = country.iso_code;
			Cookies.set('country_name', country.names.en);
			cookiecountry_name = country.names.en;
			var country_is_supported = supported_countries.indexOf(country.iso_code) > -1 || cookiecountry_code === current_site;
			var shipping_motivator = $('[data-countrycode='+cookiecountry_code+']').data('shippingmotivator');
			// $('.selected-country').html(cookiecountry_name);
			$('.shipping_motivator').html(shipping_motivator);
			if (country_is_supported) {
				return;
			} else {
				$('.cwarning-container').show();
				$('body').addClass('no-scroll');
			}
    });
  } else {
		$('[data-countrycode=' + current_site + ']').addClass('active');
		// $('.selected_country').html(cookiecountry_name);
		var country_is_supported = supported_countries.indexOf(cookiecountry_code) > -1 || cookiecountry_code === current_site;
		var shipping_motivator = $('[data-countrycode='+cookiecountry_code+']').data('shippingmotivator');
		// $('.selected-country').html(cookiecountry_name);
		$('.shipping_motivator').html(shipping_motivator);
		if (country_is_supported || !redirect) {
			Cookies.set('cookiecountry_selected', 'yes');
		} else {
			$('.cwarning-container').show();
			$('body').addClass('no-scroll');
		}
	}

	

	$('.cwarning-container').on('click', function(evt) {
		if($(evt.target).hasClass('cwarning-container')) {
			Cookies.set('country_redirect', "false");
			$('.cwarning-container').hide();
			$('body').removeClass('no-scroll');
		}
	});

	$('.country-currency-clicker').on('click', function (evt) {
		if (evt.target.dataset.siteCode) {
			// If they're hitting a link to another site then we don't need to do anything and just click the link :)
			return;
		}
		$(this).find('.country-list').toggle();
		return false;
	});

	$('.cwarning_button').on('click', function(evt) {
		evt.preventDefault();
		if ($(this).data('siteCode') === current_site) {
			Cookies.set('country_redirect', "false")
			$('.cwarning-container').hide();
			$('body').removeClass('no-scroll');
		} else {
			var pathname = window.location.pathname;
			var url = $(this).attr('href');
			/*
			* Remove query strings before redirecting.
			* Make sure there is no trailing slash on href URL as location.pathname always has one.
			*/
			var sanitizedURL = url.split('?')[0].replace(/\/$/, "");
			var regionUrl = decorateURL(sanitizedURL+pathname);

			ga('send', 'event', 'International Popup', 'Clicked redirect button');

			window.location.href = regionUrl;
		}


	});


	function decorateURL(url) {
		let transformedUrl = url;
		try {
			ga(function() {
				var trackers = ga.getAll();
				linkerParam = trackers[0].get('linkerParam');
			});

			transformedUrl = url + '?' + linkerParam;
		} catch (e) {}
		return transformedUrl;
	}

});
</script>


