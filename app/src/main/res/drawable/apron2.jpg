<script>
    // ajax add to cart requirements
    window.deferredFunctions.push(function() {

      jQuery( ".icon-popup-close" ).on('click', function() {
          jQuery('.cd-panel-cart').removeClass('is-visible');
          jQuery('body').removeClass('cart--open');
          return false;
        });

      jQuery( ".cart-open" ).on('click', function() {
        jQuery('.cd-panel-cart').addClass('is-visible');
        jQuery('body').addClass('cart--open');
        return false;
      });

      jQuery('.cart-overlay').on('click', function() {
        jQuery('.cd-panel-cart').removeClass('is-visible');
        jQuery('body').removeClass('cart--open');
      });

      $('#gift_wrap').on('change', function() {
        doGiftBox($('#gift_wrap'), $('#gift_wrap_message'), "");
