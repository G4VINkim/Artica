<script>
  const introCadPopupTimeout = 3 * 1000 || 1000;
  const introCadPopupShowOnce = true;
  window.addEventListener('load', loadCadIntroPopup);

  function loadCadIntroPopup() {
    const $body = document.querySelector('body');
    const $cadPopup = $body.querySelector('.js-cad-popup');

    if (!$cadPopup) {
      return
    }

    if ( window.geoip2 ) {
      window.geoip2.country(function(response) {
        const countryCode = response.country.iso_code;
         if (countryCode === "CA") {
           if (introCadPopupShowOnce) {
             let isPopup = cadIntroPopup.getFromStorage();
             if (!isPopup) {
               cadIntroPopup.openPopup($body, introCadPopupTimeout, $cadPopup);
               cadIntroPopup.clickOverlay($body)
               cadIntroPopup.addToStorage()
             }
           } else {
             cadIntroPopup.removeFromStorage();
             cadIntroPopup.openPopup($body, introCadPopupTimeout, $cadPopup);
             cadIntroPopup.clickOverlay($body)
           }
         }
      });
    }
  }

  const cadIntroPopup = {
    key: 'overdose_cadIntroPopup',
    openPopup(body, delay, popup){
      popup.removeAttribute('style');
      setTimeout( function () {
        body.classList.add('cad-popup-open');
      }, delay);
    },
    closePopup(body) {
      body.classList.remove('cad-popup-open');
    },
    clickOverlay(body) {
      body.addEventListener('click', function (event) {
        const target = event.target;

        if (target.classList.contains('js-cad-popup__overlay') || target.classList.contains('js-cad-popup__close')) {
          cadIntroPopup.closePopup(body);
        }
      });
    },
    addToStorage() {
      localStorage.setItem(this.key, true);
    },
    getFromStorage() {
      return localStorage.getItem(this.key);
    },
    removeFromStorage() {
      if(this.getFromStorage()) {
        localStorage.removeItem(this.key)
      }
    }
  }
</script>

