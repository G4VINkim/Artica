                    applyUnit($cells);
                }

            });

        });

        // measurement conversions
        var conversion = 2.54;

        $('[data-unit]').on("click", function(e) {

          if ($(this).attr('data-unit') == "cm" && $container.attr('data-size-unit') == "in") {
            // convert to cm
            $cells.each( function(e) {
              var self = $(this);
              if ($(this).text().includes('x')) {
                var content = $(this).text().split("x");
                self.text("");
                content.forEach(function(num, index, array) {
                  if (num != "" && !isNaN(num)) {
                    var prev_text = self.text();
                    if (prev_text == "" || index !== array.length-1) {
                      self.text(prev_text + Math.round(parseInt(num) * conversion) + "x");
                    } else {
                      self.text(prev_text + Math.round(parseInt(num) * conversion));
                    }
                    $container.attr('data-size-unit', 'cm');
                    $('[data-unit="in"]').removeAttr('data-unit-selected');
                    $('[data-unit="cm"]').attr('data-unit-selected', 'true');
                  }
                });
              } else {
                var content = $(this).text().replace(/\s/g, "").replace('cm', "").replace('in', "");
                if (content != "" && !isNaN(content)) {
                  $(this).text(Math.round(parseInt($(this).text()) * conversion))
                  $container.attr('data-size-unit', 'cm')
                  $('[data-unit="in"]').removeAttr('data-unit-selected');
                  $('[data-unit="cm"]').attr('data-unit-selected', 'true');
                }
              }
            })
            applyUnit($cells)

          } else if ($(this).attr('data-unit') == "in" && $container.attr('data-size-unit') == "cm") {
            // convert to inches
            $cells.each( function(e) {
              var self = $(this);
              if ($(this).text().includes('x')) {
                var content = $(this).text().split("x");
                self.text("");
                content.forEach(function(num, index, array) {
                  if (num != "" && !isNaN(num)) {
                    var prev_text = self.text();
                    if (prev_text == "" || index !== array.length-1) {
                      self.text(prev_text + Math.round(parseInt(num) / conversion) + "x");
                    } else {
                      self.text(prev_text + Math.round(parseInt(num) / conversion));
                    }
                    $container.attr('data-size-unit', 'in');
                    $('[data-unit="cm"]').removeAttr('data-unit-selected');
                    $('[data-unit="in"]').attr('data-unit-selected', 'true');
                  }
                });
                //content.forEach(num => console.log(num));
              } else {
                var content = $(this).text().replace(/\s/g, "").replace('cm', "").replace('in', "");
                if (content != "" && !isNaN(content)) {
                  $(this).text(Math.round(parseInt($(this).text()) / conversion))
                  $container.attr('data-size-unit', 'in')
                  $('[data-unit="cm"]').removeAttr('data-unit-selected');
                  $('[data-unit="in"]').attr('data-unit-selected', 'true');
                }
              }
            })
            applyUnit($cells)
          }

        });

        /*
         * applyUnit() - appends the measurement unit type to the numbers in the sizes table
         */
        function applyUnit($cells) {
          $cells.each( function() {
            var content = $(this).text().replace(/\s/g, "");
            if (content != "" && !isNaN(content)) {
              $(this).html(content + $container.attr('data-size-unit'))
            }
          })
        }

        $('.sizechart-trigger-specific').on('click', function () {
          var handle = $(this).data('handle');
          doSizeChartFromHandle(handle);
          return false;
        })

        /*
         * open the size chart on a specific handle
         */
        function doSizeChartFromHandle(handle) {
          if (handle != "")
          {
            var content_items = $('.size_chart_contents_div');
            var nav_items = $('.size_chart_contents_navli');
            content_items.removeClass('active');
            nav_items.removeClass('active');
            $('*[data-schandle="'+handle+'"]').addClass('active');
           }  
          $.featherlight($('.sizechart-container'), {
            afterOpen: function(event) {
              // Size chart Togglr
              $('.featherlight .sizechart_contents_nav li a').togl({
                "slaves"   : '.featherlight .sizechart_contents_inner > div',
                "grouped"  : true,
                "neveroff"  : true,
                "autoinit" : !true
              })

              // Apply default measurment units to table cells
              $container = $('.featherlight .sizechart-container')
              $cells = $container.find('tr').not(':first-child').find('td').not(':first-child')
              applyUnit($cells)
            }
          })                   
        }
  });
</script>


