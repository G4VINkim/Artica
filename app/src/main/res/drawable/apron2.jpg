          var params = {
            type: 'POST',
            url: '/cart/update.js',
            data: $('form[action="/cart"]').serialize(),
            dataType: 'json',
            async: true
          };
          $.ajax(params);
        });
      }

      dodeletebuttons();
      function dodeletebuttons()
      {

        // submit form
        jQuery( ".submitminicart" ).click(function() {
          ga(function (tracker) {
            var linkerParam = tracker.get('linkerParam');
            document.cookie = '_shopify_ga=' + linkerParam + '; ' + 'path=/';
          });
          jQuery( "#minicart" ).submit(); // this is the form
          return false;
        });

        // delete an item
        jQuery(".cartitemdelete").click(function(){
          var variant_id = jQuery(this).attr('rel');
          var params = {
            type: 'POST',
            url: '/cart/change.js',
            data: { line: variant_id, quantity: 0 },
            dataType: 'json',
            async: true,
            success: function (data)
            {
              //console.log('item_count');
              //console.log(data);
             // console.log(data.item_count);
              CartJS.getCart();
              checkGiftBox();
            },
            error: function (data)
            {
             // console.log('error');
             // console.log(data);
            }
          };
          $.ajax(params);
          return false;
        });

        // add one
        jQuery(".cartitemupdateplus").click(function(){
        //  console.log('cartitemupdateplus');
          var variant_id = jQuery(this).attr('rel');
          var newvalue = parseInt(jQuery(this).data('oldvalue'))+1;
          var params = {
            type: 'POST',
            url: '/cart/change.js',
            data: { line: variant_id, quantity: newvalue },
            dataType: 'json',
            async: true,
            success: function (data)
            {
              //console.log('item_count');
              //console.log(data);
             //console.log(data.item_count);
              CartJS.getCart();
              checkGiftBox();
              showCartUpdate();
            },
            error: function (data)
            {
              //console.log('error');
             //console.log(data);
            }
          };
          $.ajax(params);
          return false;
