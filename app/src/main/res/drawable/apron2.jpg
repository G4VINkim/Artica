  function doGiftBox(giftwrapcheckbox, giftwrapmessagebox, f) {
    var giftwrapcheckbox = giftwrapcheckbox;
    var giftwrapfreevariantid = giftwrapcheckbox.data('freeid');
    var giftwrappaidvariantid = giftwrapcheckbox.data('paidid');
    var giftwrapmessage = giftwrapmessagebox.val();
    var ischecked = giftwrapcheckbox.is(":checked");

    // show or hide message
    if(ischecked) {
      //console.log('checked free='+giftwrapfreevariantid+' paid='+giftwrappaidvariantid);
      $('.cart-gift-message').addClass('cart-gift-message-shown');
      $('.cart-gift').addClass('cart-gift--open');
      $('.cd-panel-content').addClass('cart-info--gift');
    } else {
     // console.log('unchecked free='+giftwrapfreevariantid+' paid='+giftwrappaidvariantid);
      $('.cart-gift-message').removeClass('cart-gift-message-shown');
      $('.cart-gift').removeClass('cart-gift--open');
      $('.cd-panel-content').removeClass('cart-info--gift');
    }

    // get cart and add item if not found
    CartJS.getCart({success: function() {
      var data = CartJS.cart;
      // console.log(data);
      var ispaidgiftfound = false;
      var isfreegiftfound = false;
      var ispaidgiftid = false;
      var isfreegiftid = false;
      if (data.total_price >= 50000) {
        var giftproductid = giftwrapfreevariantid;
      } else {
        var giftproductid = giftwrappaidvariantid;
      }

      if (data.items) {
        jQuery.each(data.items, function(i, item) {
          if (item.variant_id == giftwrappaidvariantid) {
            ispaidgiftfound = true;
            ispaidgiftid = item.id;
          }
        });
        jQuery.each(data.items, function(i, item) {
          if (item.variant_id == giftwrapfreevariantid) {
            isfreegiftfound = true;
            isfreegiftid = item.id;
          }
        });
        addFreeGiftCard(data.total_price, ispaidgiftfound, isfreegiftfound, ischecked, giftwrapmessage, giftwrapfreevariantid, giftwrappaidvariantid, ispaidgiftid, isfreegiftid, f);
      } else {
        addFreeGiftCard(data.total_price, ispaidgiftfound, isfreegiftfound, ischecked, giftwrapmessage, giftwrapfreevariantid, giftwrappaidvariantid, ispaidgiftid, isfreegiftid, f);
      }

    } });
  }

