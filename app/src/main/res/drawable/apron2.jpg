        //var giftwrapmessage = $('#gift_wrap_message').val();
        var giftwrapmessage = $('.gift_wrap_message_textarea').val();
        updateGiftWrapMessage(encodeURIComponent(giftwrapmessage));
        $saveMessageBtn.text('Saved');
        setTimeout(function(){
          $saveMessageBtn.text('Save message');
        }, 2000);
        return false;
      });

    

      CartJS.getCart();
      // cart requirement
      if ($('body').hasClass('template-cart')) {
        /* This auto-saves cart attribute and cart note.
