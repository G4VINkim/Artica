        // sort by name alphabetically
        sizes.sort(function(a, b) {
          var nameA = a.title.toUpperCase()
          var nameB = b.title.toUpperCase()
          if (nameA < nameB) {
            return -1
          }
          if (nameA > nameB) {
            return 1
          }
          // names must be equal
          return 0
        });

        // Build html
        for (size of sizes) {
          // var handlised_title = size.title.toLowerCase().replace(' ', '-')

          if (size.img) {
            var image = '<img data-src="' + size.img + '">'
